Codigo 
#include <iostream>
using namespace std;

int main(){ 
// Definição das variáveis
    int a=2,z;

    // Expressão algébrica: z = 2%2 + 2*2/2
    z = a%a + a*a/a;
    
    // Saída do resultado
    cout << "O resultado da expressão z =2%2 + 2*2/2;  é: " << z << endl;
    
    return 0;

}
