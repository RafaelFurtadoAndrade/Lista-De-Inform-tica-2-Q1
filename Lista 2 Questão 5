#include <iostream>
using namespace std;
int main () {
	//Definição das variáveis
	int a, b, z;
	
	//Solicitação para o usuário inserir os valores
	cout<< "digite o valor da largura";
	cin >>a;
	
	cout<< "digite o valor do comprimento";
	cin >>b;
	
	//Produto de a*b
	z=a*b;
	
	//Resultado
	cout << "O resultado da area é:" << z << endl;
	
	return 0;
	
	
	
}
digite o valor da largura5
digite o valor do comprimento10
O resultado da area Ú:50

